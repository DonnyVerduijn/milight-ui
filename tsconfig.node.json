{
  "compilerOptions": {
    "composite": true,
    "skipLibCheck": true,
    "module": "ESNext",
    "moduleResolution": "bundler",
    "allowSyntheticDefaultImports": true,
    "strict": true,

    "paths": {
      "test/*": ["./test/*"],
      "scripts/*": ["./scripts/*"],
    }
  },
  "include": ["vite.config.ts", "scripts", "test"],
}
