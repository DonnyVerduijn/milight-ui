{
  "name": "milight-ui",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --port 4173 --open",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "vitest",
    "analyze": "yarn build && cd .analyzer && npx --yes http-server -p 8091 -o",
    "clean": "rm -rf dist .vite-inspect .analyzer .swc yarn-error.log",
    "reset": "yarn clean && rm -rf node_modules /home/vscode/.cache",   
    "act": "act -P ubuntu-latest=-self-hosted -j build --cache-server-addr 0.0.0.0"
  },
  "dependencies": {
    "@effect/schema": "^0.66.10",
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@mui/icons-material": "^5.15.15",
    "@mui/material": "^5.15.15",
    "dayjs": "^1.11.11",
    "effect": "^3.0.3",
    "fast-check": "^3.18.0",
    "jotai": "^2.8.0",
    "mobx": "^6.12.3",
    "mobx-react-lite": "^4.0.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-use": "^17.5.0",
    "remeda": "^1.61.0",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@swc/plugin-emotion": "^3.0.0",
    "@types/node": "^20.12.7",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.7.0",
    "@typescript-eslint/parser": "^7.7.0",
    "@vitejs/plugin-react-swc": "^3.6.0",
    "axios": "^1.6.8",
    "babel-plugin-import": "^1.13.8",
    "chalk": "^5.3.0",
    "earl": "^1.2.1",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^8.0.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "eslint-plugin-unused-imports": "^3.1.0",
    "eslint-plugin-vitest": "^0.5.4",
    "hotscript": "^1.0.13",
    "prettier": "^3.2.5",
    "rollup-plugin-visualizer": "^5.12.0",
    "tsx": "^4.7.3",
    "type-fest": "^4.17.0",
    "typescript": "^5.2.2",
    "vite": "^5.2.0",
    "vite-plugin-checker": "^0.6.4",
    "vite-plugin-inspect": "^0.8.4",
    "vitest": "^1.5.2"
  }
}
