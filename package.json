{
  "name": "milight-ui",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --port 4173",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "inspect": "yarn build && cd .vite-inspect && npx http-server -p 8090 -o",
    "analyze": "yarn build && cd .analyzer && npx http-server -p 8091 -o",
    "clean": "rm -rf dist .vite-inspect .analyzer"
  },
