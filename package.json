    "@mui/material": "^5.15.15",
    "effect": "^3.0.3",
    "fp-ts": "^2.16.5",
    "jotai": "^2.8.0",
    "mobx": "^6.12.3",
    "mobx-react-lite": "^4.0.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-use": "^17.5.0",
    "remeda": "^1.61.0",
    "zustand": "^4.5.2"
  },
