{
  "name": "milight-ui",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "postinstall": "patch-package",
    "dev": "vite --port 4173",
    "build": "rm -rf dist && tsc && vite build",
    "lint": "eslint . --ext js,cjs,ts,tsx",
    "lint:style": "stylelint \"src/**/*.css\" \"src/**/*.tsx\"",
    "lint:fix": "yarn lint --fix",
    "preview": "yarn build && NODE_NO_WARNINGS=1 tsx -e \"import { viewStatic } from './scripts/browser'; viewStatic('./dist');\"",
    "test": "vitest --ui",
    "test:e2e": "npx --yes tsx --watch ./.devcontainer/launch.ts",
    "analyze": "yarn build && NODE_NO_WARNINGS=1 tsx -e \"import { viewStatic } from './scripts/browser'; viewStatic('./.analyzer');\"",
    "clean": "rm -rf dist .vite-inspect .analyzer .swc yarn-error.log",
    "reset": "yarn clean && rm -rf node_modules /home/vscode/.cache",
    "ci:local": "act -P ubuntu-latest=-self-hosted -j build --cache-server-addr 0.0.0.0",
    "dep:stats": "npx --yes tsx ./scripts/dep-stats.ts"
  },
  "dependencies": {
    "@effect/schema": "^0.66.10",
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@mui/icons-material": "^5.15.15",
    "@mui/material": "^5.15.15",
    "@trpc/client": "^10.45.2",
    "@trpc/server": "^10.45.2",
    "culori": "^4.0.1",
    "dayjs": "^1.11.11",
    "effect": "^3.0.3",
    "fast-check": "^3.18.0",
    "jotai": "^2.8.0",
    "mobx": "^6.12.3",
    "mobx-react-lite": "^4.0.7",
    "mobx-utils": "^6.0.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router": "^6.23.0",
    "react-use": "^17.5.0",
    "remeda": "^1.61.0",
    "uuid": "^9.0.1",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "@puppeteer/browsers": "^2.2.3",
    "@swc/plugin-emotion": "^3.0.0",
    "@testing-library/react": "^15.0.5",
    "@types/cssbeautify": "^0.3.5",
    "@types/http-server": "^0.12.4",
    "@types/node": "^20.12.7",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^7.7.0",
    "@typescript-eslint/parser": "^7.7.0",
    "@vitejs/plugin-react-swc": "^3.6.0",
    "@vitest/ui": "^1.5.3",
    "autoprefixer": "^10.4.19",
    "axios": "^1.6.8",
    "chalk": "^5.3.0",
    "cssbeautify": "^0.3.1",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^8.0.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "eslint-plugin-typescript-sort-keys": "^3.2.0",
    "eslint-plugin-unused-imports": "^3.1.0",
    "eslint-plugin-vitest": "^0.5.4",
    "happy-dom": "^14.7.1",
    "hotscript": "^1.0.13",
    "http-server": "^14.1.1",
    "meow": "^13.2.0",
    "msw": "^1.3.3",
    "msw-trpc": "^1.3.4",
    "patch-package": "^8.0.0",
    "postcss": "^8.4.38",
    "postcss-styled-syntax": "^0.6.4",
    "postinstall-postinstall": "^2.1.0",
    "prettier": "^3.2.5",
    "puppeteer": "^22.7.1",
    "puppeteer-core": "^22.7.1",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-user-preferences": "^2.4.1",
    "rollup-plugin-visualizer": "^5.12.0",
    "stylelint": "^16.5.0",
    "stylelint-config-recommended": "^14.0.0",
    "stylelint-config-standard": "^36.0.0",
    "stylelint-order": "^6.0.4",
    "ts-plugin-sort-import-suggestions": "^1.0.4",
    "tsx": "^4.9.3",
    "type-fest": "^4.17.0",
    "typescript": "^5.4.5",
    "vite": "^5.2.0",
    "vite-plugin-checker": "^0.6.4",
    "vite-plugin-inspect": "^0.8.4",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^1.5.3"
  },
  "workspaces": [
    "packages/*"
  ],
  "browserslist": [
    "defaults"
  ]
}
